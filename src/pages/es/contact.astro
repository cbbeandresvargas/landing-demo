---
import ContactForm from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contacto - Andres Vargas - Desarrollador Full-Stack",
	description:
		"Contacta a Andres Vargas para proyectos de desarrollo de software. Email, teléfono, ubicación y disponibilidad.",
	canonical: "https://mintaka.co/es/contact/",
};
---

<BaseLayout seo={seo}>
	<ContactForm />
</BaseLayout>
