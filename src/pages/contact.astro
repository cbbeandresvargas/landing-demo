---
import ContactForm from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact Andres Vargas - Full-Stack Developer",
	description:
		"Get in touch with Andres Vargas for software development projects. Email, phone, location and availability.",
	canonical: "https://mintaka.co/contact/",
};
---

<BaseLayout seo={seo}>
	<ContactForm />
</BaseLayout>
